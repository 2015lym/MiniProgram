<!--pages/echarts-detail/run-number/run-number.wxml-->
<view class="top-back-view">
  <view style="height: 30px"></view>
  <view class="echarts-card">
    <text class="view-title">7天运行总次数\n<text class="view-subtitle">{{elevatorData.NumberOfRuns}}次</text>
    </text>
    <echarts2 echartsData="{{elevatorData.NearlyDaysModels}}"></echarts2>
  </view>
</view>

<view class="list-item-view" style="margin-top: 150px">
  <view class="list-item-view-left">
    <image src="../../../resource/images/elevator-echarts.png" class="list-item-view-image"></image>
    <text>平均运行次数</text>
  </view>
  <view>{{elevatorData.AverageNumberOfRuns}}次</view>
</view>

