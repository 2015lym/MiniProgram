<!--pages/insurance/pay-insurance-order/cancel-insurance-list/cancel-insurance-list.wxml-->

<i-panel title="电梯信息">
  <i-input right value="{{ listData.LiftNum }}" title="电梯编号" disabled/>
  <i-input right value="{{ listData.CertificateNum }}" title="注册代码" disabled/>
  <i-input right value="{{ listData.UseDepartment }}" title="使用单位" disabled/>
</i-panel>
<i-panel title="保险信息">
  <i-input right value="{{ listData.InsuranceCarriers }}" title="承保公司" disabled/>
  <i-input right value="{{ listData.PolicyNumber }}" title="保单号" disabled/>
  <i-input right value="{{ listData.InsuranceBeginTime }}" title="保险起期" disabled/>
  <i-input right value="{{ listData.InsuranceEndTime }}" title="保险止期" disabled/>
</i-panel>

<i-button bind:click="cancelInsurance" type="primary">退保</i-button>

<i-toast id="toast" />